{
  "description": "Auto-installs and updates the websearch CLI binary from GitHub releases",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "${CLAUDE_PLUGIN_ROOT}/hooks/scripts/check-websearch.sh",
            "timeout": 30
          }
        ]
      }
    ]
  }
}
